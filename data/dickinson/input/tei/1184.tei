        <fileDesc>
            <titleStmt>
                <title>Bereaved of all I went abroad </title>
            </titleStmt>
            <publicationStmt>
                <p>The Poems of Emily Dickinson, Variorum Edition, Franklin, 1998</p>
            </publicationStmt>
            <sourceDesc>
                <p>Emily Dickinson Archive</p>
            </sourceDesc>
        </fileDesc>
