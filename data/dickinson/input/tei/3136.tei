    <text>
        <body>
            <div type="transcript" id="J676">
                <lg type="stanza">
                    <l>Least Bee that brew --</l>
                    <l>A Honey's Weight</l>
                    <l>The Summer multiply --</l>
                    <l>Content Her smallest fraction help</l>
                    <l>The Amber Quantity --</l>
                </lg>

            </div>     
        </body>
