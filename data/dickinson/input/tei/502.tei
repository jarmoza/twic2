    <teiHeader>
        <fileDesc>
            <titleStmt>
                <title>He showed me hights I never saw</title>
            </titleStmt>
            <publicationStmt>
                <p>The Poems of Emily Dickinson, Variorum Edition, Franklin, 1998</p>
            </publicationStmt>
            <sourceDesc>
