    <teiHeader>
        <fileDesc>
            <titleStmt>
                <title>It was a quiet way </title>
            </titleStmt>
            <publicationStmt>
                <p>The Poems of Emily Dickinson, Variorum Edition, Franklin, 1998</p>
            </publicationStmt>
