    <teiHeader>
        <fileDesc>
            <titleStmt>
                <title>It dont sound so terrible quite as it did </title>
            </titleStmt>
            <publicationStmt>
                <p>The Poems of Emily Dickinson, Variorum Edition, Franklin, 1998</p>
            </publicationStmt>
            <sourceDesc>
                <p>Emily Dickinson Archive</p>
            </sourceDesc>
        </fileDesc>
