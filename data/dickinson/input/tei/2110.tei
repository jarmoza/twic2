    <teiHeader>
        <fileDesc>
            <titleStmt>
                <title>You cannot make remembrance grow </title>
            </titleStmt>
            <publicationStmt>
                <p>The Poems of Emily Dickinson, Variorum Edition, Franklin, 1998</p>
            </publicationStmt>
            <sourceDesc>
