    <text>
        <body>
            <div type="transcript" id="J225">
                <lg type="stanza">
                    <l>Jesus! thy Crucifix</l>
                    <l>Enable thee to guess</l>
                    <l>The smaller size!</l>
                </lg>
                <lg type="stanza">
                    <l>Jesus! thy second face</l>
                    <l>Mind thee in Paradise</l>
                    <l>Of our's!</l>
                </lg>

            </div>     
        </body>
    </text>
