        <fileDesc>
            <titleStmt>
                <title>I&#x27;m nobody!  Who are you?</title>
            </titleStmt>
            <publicationStmt>
                <p>Poems by Emily Dickinson: Second Series, Higginson and Todd, 1891</p>
            </publicationStmt>
            <sourceDesc>
                <p>Emily Dickinson Archive</p>
            </sourceDesc>
        </fileDesc>
