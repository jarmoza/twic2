        <fileDesc>
            <titleStmt>
                <title>It can&#x27;t be summer,  that got through;</title>
            </titleStmt>
            <publicationStmt>
                <p>Poems by Emily Dickinson: Second Series, Higginson and Todd, 1891</p>
            </publicationStmt>
            <sourceDesc>
                <p>Emily Dickinson Archive</p>
            </sourceDesc>
        </fileDesc>
