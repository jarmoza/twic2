    <teiHeader>
        <fileDesc>
            <titleStmt>
                <title>There is a June when corn is cut </title>
            </titleStmt>
            <publicationStmt>
                <p>The Poems of Emily Dickinson, Variorum Edition, Franklin, 1998</p>
            </publicationStmt>
