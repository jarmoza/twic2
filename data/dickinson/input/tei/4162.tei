    <text>
        <body>
            <div type="transcript" id="J1702">
                <lg type="stanza">
                    <l>To-day or this noon</l>
                    <l>She dwelt so close</l>
                    <l>I almost touched her</l>
                    <l>To-night she lies</l>
                    <l>Past neighborhood</l>
                    <l>And bough and steeple</l>
                    <l>Now past surmise</l>
                </lg>

            </div>     
        </body>
    </text>
