    <teiHeader>
        <fileDesc>
            <titleStmt>
                <title>He fumbles at your soul </title>
            </titleStmt>
            <publicationStmt>
                <p>The Poems of Emily Dickinson, Variorum Edition, Franklin, 1998</p>
            </publicationStmt>
