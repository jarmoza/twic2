    <teiHeader>
        <fileDesc>
            <titleStmt>
                <title>Art thou the thing I wanted? </title>
            </titleStmt>
            <publicationStmt>
                <p>The Poems of Emily Dickinson, Variorum Edition, Franklin, 1998</p>
            </publicationStmt>
            <sourceDesc>
