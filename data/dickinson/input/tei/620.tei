    <teiHeader>
        <fileDesc>
            <titleStmt>
                <title>I years had been from home </title>
            </titleStmt>
            <publicationStmt>
                <p>The Poems of Emily Dickinson, Variorum Edition, Franklin, 1998</p>
            </publicationStmt>
            <sourceDesc>
                <p>Emily Dickinson Archive</p>
            </sourceDesc>
        </fileDesc>
