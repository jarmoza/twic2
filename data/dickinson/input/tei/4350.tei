    <teiHeader>
        <fileDesc>
            <titleStmt>
                <title>I never lost as much but twice,</title>
            </titleStmt>
            <publicationStmt>
                <p>Poems by Emily Dickinson, Todd and Higginson, 1890</p>
            </publicationStmt>
            <sourceDesc>
                <p>Emily Dickinson Archive</p>
            </sourceDesc>
        </fileDesc>
    </teiHeader>
    <text>
        <body>
            <div type="transcript" id="P90-115">
                <lg type="stanza">
                    <l>I never lost as much but twice,</l>
                    <l>And that was in the sod;</l>
                    <l>Twice have I stood a beggar</l>
                    <l>Before the door of God!</l>
                </lg>
                <lg type="stanza">
                    <l>Angels, twice descending,</l>
                    <l>Reimbursed my store.</l>
                    <l>Burglar, banker, father,</l>
                    <l>I am poor once more!</l>
                </lg>
                <lg type="stanza">
                </lg>

            </div>     
        </body>
    </text>
