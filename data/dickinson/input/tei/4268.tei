            <titleStmt>
                <title>WITH A FLOWER</title>
            </titleStmt>
            <publicationStmt>
                <p>Poems by Emily Dickinson, Todd and Higginson, 1890</p>
            </publicationStmt>
            <sourceDesc>
                <p>Emily Dickinson Archive</p>
            </sourceDesc>
        </fileDesc>
